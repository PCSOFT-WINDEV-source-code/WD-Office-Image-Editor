  internal_properties : CAAAAAgAAAAMDdCm54j1DSAvd9Y0phiUDMnQhLtOAYKY9HNjgO0EcwV6HfJZ+QnqlPeqPWpbhoDFaTNs28RARsR/gKjNp7Tw1tWUa8/XfbxE71qpKS7XSVpaAFo3S4+cd0/pxgHCLpbQ4yECME+W
  type_code : 1
  p_codes :
   -
     code : |1+
      Procdure MyWindow()
   -
     code : |1+
      
      // Code specific to creation
      DocumentAction.CreateDocument = __CreateNewImage
      
      // Code specific to opening
      DocumentAction.OpenDocument = __OpenImage
      
      // Code specific to backup
      DocumentAction.SaveDocument = __SaveImage
      
      	INTERNAL PROCEDURE __CreateNewImage() : boolean
      	
      	// Creates the image
      	if ControlExist("BTN_NEW") then
      		// From the "New" control of ribbon (to get the control menu)
      		ExecuteProcess("BTN_NEW", trtClick)
      	else
      		// From the function if the ribbon control is not accessible (to get a default blank page)
      		IE_Editor.create(picImage, 1024, 768)
      	end
      	
      	// Cancels the indication of modifications
      	IE_Editor..Modified = False
      	
      	result True
      	
      	end
      
      	INTERNAL PROCEDURE __OpenImage( sPath is string, LOCAL xDocPos is numeric <useful>= -1) : boolean
      	
      	// Loads the file in the editor
      	if IE_Editor.open(sPath) = False then result False
      	
      	if ControlExist("STC_ReadOnly") = True then
      		// Applies the 'Read-only' mode only if the Read-only caption is displayed after the load
      		ChangeReadOnlyMode( {"STC_ReadOnly", indControl}..Visible )
      	end
      	
      	result True
      	
      	end
      
      	INTERNAL PROCEDURE __SaveImage( sPath is string ) : boolean
      	
      	// Saves the image in the file
      	if IE_Editor.save(sPath) = False then result False
      	
      	// If "Save as" is performed on a read-only image
      	if gbReadOnlyMode then
      		// Forces the image that was saved to be reloaded in order to enable the tools
      		MenuOpenDocument(sPath, True)
      	end
      	
      	result True
      	
      	end
      
      
      end:
      
      // Initializes the ribbon
      InitializeRibbon("RUBAN_WINDEVFAA_EditeurImages", Null)
      
      // Blank document by default
      // (only on the initial one: on the aliases, the procedure is automatically called)
      if MyWindow..FullName ~= "WIN_Main" then
      	// Create the default blank page
      	IE_Editor.create(picImage, 1024, 768)
