      // Summary: Triggers the document backup
      // Syntax:
      //[ <Result> = ] MenuSaveDocument ([<sFilePath> is string [, <bForceBrowseSelection> is boolean]])
      //
      // Parameters:
      //	sFilePath (UNICODE string): Path where the document will be saved (by default, current path of opened document)
      //	bForceBrowseSelection (boolean - default value=0): True if the selection of backup file must be forced
      // Return value:
      // 	integer: 0 or 1 depending on whether the document was saved, -1 if the [Save as] dialog was triggered
      //
      // Automatic process of exceptions:   run CASE EXCEPTION:
      //
      
      Procdure MenuSaveDocument(local sFilePath is string = gsDocumentPath, local bForceBrowseSelection is boolean = False) : int
      
      // The document was not saved yet or it was modified in read-only
      if sFilePath = "" _or_ (gsDocumentPath = sFilePath _and_ gbReadOnlyMode) then
      	
      	if bForceBrowseSelection = False then
      		
      		// If the displayed plane is not a menu plane
      		if MyWindow..Plane < PLAN_MENU then
      			
      			// Forces the menu opening
      			BTN_FILE_MENU.ExecuteProcess(trtClick)
      		end
      		
      		// Triggers the 'Save as' process
      		BTN_MENU_SAVE_AS.ExecuteProcess(trtClick)		
      		result -1
      	else
      		
      		// Opens the file picker
      		sFilePath = OpenFilePicker()		
      		if sFilePath = "" then
      			result False
      		end
      	end		
      end
      
      // Triggers the backup
      if DocumentAction.SaveDocument = Null _or_ DocumentAction.SaveDocument( sFilePath ) = True then
      	
      	// End of modifications and end of read-only		
      	gbModifiedDocument = False
      	gbReadOnlyMode = False
      	
      	// Stores the path
      	gsDocumentPath = sFilePath
      	
      	// Update
      	MenuDisplayInfo( gsDocumentPath )
      	
      	// Stores the document in the MRU
      	MRUAdd( sFilePath, gxDocPositionne )
      	
      	// Displays a toast
      	ToastDisplay("Document saved", toastShort, vaMiddle)
      	
      	// Closes the file menu
      	FileMenuClose()
      	
      	result True
      end
      
      result False
      
      case exception:
      dbgAssert( False, ExceptionInfo )
      result False
