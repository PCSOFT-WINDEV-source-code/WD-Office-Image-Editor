      // Summary: <specify the action of the procedure>
      // Syntax:
      //MenuDisplayInfo (<sPath> is string)
      //
      // Parameters:
      //	sPath (ANSI string): <specify the role of sPath>
      // Return value:
      // 	None
      //
      Procdure MenuDisplayInfo(local sPath is string)
      
      bReadOnly is boolean = ( BTN_MENU_NEW..Visible = False _and_ BTN_MENU_SAVE..Visible = False )
      
      // In Creation
      if sPath = "" _or_ fFileExist(sPath) = False then
      	
      	STC_INFO_SIZE = "Not saved yet"
      	STC_INFO_MODIFICATIONDATE = ""
      	STC_INFO_CREATIONDATE = DateToString(Today, maskDateRelativeDuration) + [", "] + TimeToString(Now, "HH:MM")
      	
      	BTN_OPEN_LOCATION..Visible = False
      	STC_TITLE_DISK_INFORMATION..Visible = False
      	
      	// Name and path of document
      	if bReadOnly then
      		
      		// No default name in read-only
      		gsNameOpenedDocument = ""
      	else
      		gsNameOpenedDocument = "Document" + SET_SETTING.gnDocumentCounter
      	end
      	gsDocumentPath = ""
      	
      else
      	
      	// Displays the formatted size
      	STC_INFO_SIZE = LengthToString( fSize(sPath) )
      	
      	// Reads the modification and creation date/time
      	dtModification is datetime = fDateTime(sPath, "", fModify)
      	dtCreation is datetime = fDateTime(sPath, "", fCreation)
      	
      	// Formats the display of dates
      	STC_INFO_MODIFICATIONDATE = DateToString(dtModification..Date, maskDateRelativeDuration) + [", "] + TimeToString(dtModification..Time, "HH:MM")
      	STC_INFO_CREATIONDATE = DateToString(dtCreation..Date, maskDateRelativeDuration) + [", "] + TimeToString(dtCreation..Time, "HH:MM")
      	
      	BTN_OPEN_LOCATION..Visible = False
      	STC_TITLE_DISK_INFORMATION..Visible = False
      	
      	// Name and path of document
      	gsNameOpenedDocument = fExtractPath(sPath, fFileName + fExtension)	
      	gsDocumentPath = sPath		
      	
      end
      
      // Update the title of the window
      MyWindow..Title = ( gsNameOpenedDocument + (gbModifiedDocument ? "*" else "") + (gbReadOnlyMode ? " ** READ-ONLY **" else "") ) + [" - "] + NoSpace(ExtractString(ProjectInfo(piProjectName), 2, "WD Office "))
